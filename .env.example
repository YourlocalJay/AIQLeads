# AIQLeads Environment Configuration

# Application Settings
APP_NAME=AIQLeads
ENVIRONMENT=development  # development, staging, production
DEBUG=True
LOG_LEVEL=INFO

# Security
SECRET_KEY=your-secret-key-here
JWT_SECRET=your-jwt-secret-here
JWT_ALGORITHM=HS256
JWT_EXPIRATION_DAYS=30

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/aiqleads
POSTGRES_USER=user
POSTGRES_PASSWORD=password
POSTGRES_DB=aiqleads

# Redis Cache
REDIS_URL=redis://localhost:6379/0
REDIS_PASSWORD=your-redis-password

# AI Services
OPENAI_API_KEY=your-openai-key
TENSORFLOW_SERVING_URL=http://localhost:8501
MODEL_WEIGHTS_PATH=/path/to/models

# Third-Party APIs
MLS_API_KEY=your-mls-api-key
ZILLOW_API_KEY=your-zillow-api-key

# Email Service
SENDGRID_API_KEY=your-sendgrid-key
EMAIL_FROM=noreply@aiqleads.com

# Payment Processing
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret
STRIPE_PRICE_ID=your-stripe-price-id

# Feature Flags
ENABLE_AI_SCORING=True
ENABLE_AUTOMATIC_MATCHING=True
ENABLE_REAL_TIME_NOTIFICATIONS=True

# Monitoring
SENTRY_DSN=your-sentry-dsn
PROMETHEUS_ENABLED=True

# Scraping Configuration
MAX_CONCURRENT_SCRAPES=10
SCRAPING_INTERVAL_MINUTES=60
USER_AGENT=AIQLeads/1.0

# API Rate Limiting
RATE_LIMIT_ENABLED=True
RATE_LIMIT_DEFAULT=100/hour

# Caching
CACHE_ENABLED=True
CACHE_TTL_SECONDS=3600

# Background Tasks
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# AWS Services (Optional)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-west-2
S3_BUCKET_NAME=aiqleads-data

# Logging
LOG_FORMAT=json
LOG_FILE_PATH=/var/log/aiqleads/app.log
ENABLE_FILE_LOGGING=True